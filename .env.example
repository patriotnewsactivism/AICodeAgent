# CodeVibe V2 Environment Variables
# Copy this file to .env and fill in your values

# Gemini API Key (get from https://ai.google.dev)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Custom API endpoint (leave empty for default Gemini)
VITE_API_ENDPOINT=

# Optional: Model name (default: gemini-2.0-flash-exp)
VITE_MODEL_NAME=gemini-2.0-flash-exp
